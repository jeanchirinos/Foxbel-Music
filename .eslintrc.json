{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "plugin:react/jsx-runtime",
        "standard",
        "prettier"
    ],
    "overrides": [],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "prefer-const": "warn",
        "no-empty": "warn",
        "no-unreachable": "warn",
        "spaced-comment": "off",
        "camelcase": "off"
    },
    "globals": {
        "React": "readonly",
        "JSX": "readonly",
        "DZ": "readonly"
    }
}